#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/G:/study abroad Japan/kun_resume/node_modules/.store/@vuepress+cli@2.0.0-rc.14/node_modules/@vuepress"
else
  export NODE_PATH="$NODE_PATH:$basedir/G:/study abroad Japan/kun_resume/node_modules/.store/@vuepress+cli@2.0.0-rc.14/node_modules/@vuepress"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@vuepress+cli@2.0.0-rc.14/node_modules/@vuepress/cli/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../../../../@vuepress+cli@2.0.0-rc.14/node_modules/@vuepress/cli/bin/vuepress.js" "$@"
fi
